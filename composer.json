{
    "name": "friendsofcat/bulk-actions",
    "description": "Bulk action support for Laravel applications.",
    "version": "1.0.0",
    "type": "library",
    "readme": "README.md",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/friendsofcat/bulk-actions/issues",
        "source": "https://github.com/friendsofcat/bulk-actions"
    },
    "require": {
        "illuminate/auth": "^7.0|^8.0",
        "illuminate/broadcasting": "^7.0|^8.0",
        "illuminate/bus": "^7.0|^8.0",
        "illuminate/container": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/events": "^7.0|^8.0",
        "illuminate/queue": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.7"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfCat\\BulkActions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FriendsOfCat\\BulkActions\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "FriendsOfCat\\BulkActions\\BulkActionsServiceProvider"
            ],
            "aliases": {
                "BulkAction": "FriendsOfCat\\BulkActions\\Facades\\BulkAction"
            }
        }
    }
}
